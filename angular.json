{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "search-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/search-app",
      "sourceRoot": "projects/search-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/search-app",
            "index": "projects/search-app/src/index.html",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/search-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/search-app/src/favicon.ico",
              "projects/search-app/src/assets"
            ],
            "styles": ["projects/search-app/src/styles.scss"],
            "scripts": [],

            "main": "projects/search-app/src/main.ts",
            "extraWebpackConfig": "projects/search-app/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "projects/search-app/webpack.prod.config.js"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "search-app:build:production",
              "extraWebpackConfig": "projects/search-app/webpack.prod.config.js"
            },
            "development": {
              "buildTarget": "search-app:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4300,
            "publicHost": "http://localhost:4300",
            "extraWebpackConfig": "projects/search-app/webpack.config.js"
          }
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "buildTarget": "search-app:build",
            "extraWebpackConfig": "projects/search-app/webpack.config.js"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/search-app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/search-app/src/favicon.ico",
              "projects/search-app/src/assets"
            ],
            "styles": ["projects/search-app/src/styles.scss"],
            "scripts": []
          }
        }
      }
    },
    "home-app": {
